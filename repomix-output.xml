          args:
            retries: 3
            # Sem NOT_FOUND para login r√°pido
            statuses: BAD_GATEWAY,GATEWAY_TIMEOUT,INTERNAL_SERVER_ERROR,SERVICE_UNAVAILABLE
            methods: GET,POST,PUT,DELETE,PATCH,OPTIONS
