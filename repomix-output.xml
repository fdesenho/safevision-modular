    this.openSnackBar(message, 'OK', 'info-snackbar', 4000, 'end', 'top');
  }

  
  private openSnackBar(
    message: string,
    action: string,
    panelClass: string,
    duration: number,
    horizontalPosition: MatSnackBarHorizontalPosition,
    verticalPosition: MatSnackBarVerticalPosition
  ) {
